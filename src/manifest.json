{
  "name": "AI SW Chrome Extension",
  "description": "Capture data from SW sites",
  "version": "1.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["alarms", "storage", "activeTab", "tabs", "debugger"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./images/get_started16.png",
      "32": "./images/get_started32.png",
      "48": "./images/get_started48.png",
      "128": "./images/get_started128.png"
    }
  },
  "icons": {
    "16": "./images/get_started16.png",
    "32": "./images/get_started32.png",
    "48": "./images/get_started48.png",
    "128": "./images/get_started128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
